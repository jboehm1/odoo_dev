<odoo>
  <record id="view_partner_form_map" model="ir.ui.view">
    <field name="name">res.partner.form.map</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
      <xpath expr="//notebook" position="inside">
        <page string="Carte">
          <group>
            <field name="latitude"/>
            <field name="longitude"/>
          </group>
          <group>
            <field name="map_url" readonly="1" widget="image_url"/>
          </group>
        </page>
      </xpath>
    </field>
  </record>
</odoo>
